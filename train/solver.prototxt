net: "train_val.prototxt"
snapshot_prefix: "snapshots/snapshot"
base_lr: 1e-02
max_iter: 1
iter_size: 1
## test_iter: 1000
## test_interval: 1000
lr_policy: "step"
gamma: 0.1
momentum: 0.9
weight_decay: 1e-05
stepsize: 10000
snapshot: 500
solver_type: ADAM
solver_mode: GPU
display: 100
debug_info: false
